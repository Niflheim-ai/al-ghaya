{
    "name": "al-ghaya-2/project",
    "description": "Al-Ghaya 2: A Gamified Learning Management System For Arabic and Islamic Studies",
    "type": "project",
    "keywords": ["lms", "gamification", "arabic", "islamic-studies", "education"],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "google/apiclient": "^2.15",
        "google/apiclient-services": "^0.340",
        "google/auth": "^1.38",
        "phpmailer/phpmailer": "^6.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "php/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan package:discover --ansi || true"
        ]
    }
}